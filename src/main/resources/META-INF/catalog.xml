<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog"
         xmlns:px="http://www.daisy.org/ns/pipeline">
  <!-- scripts -->
  <uri name="http://www.celia.fi/pipeline/modules/braille/dtbook-to-pef.xpl" uri="../xml/dtbook-to-pef.xpl"
       px:extends="http://www.daisy.org/pipeline/modules/braille/dtbook-to-pef/dtbook-to-pef.xpl"
       px:content-type="script" px:id="celia:dtbook-to-pef"/>
  <!-- library -->
  <uri name="http://www.celia.fi/pipeline/modules/braille/library.xpl" uri="../xml/library.xpl"/>
  <!-- CSS -->
  <uri name="http://www.celia.fi/pipeline/modules/braille/default.scss"
       uri="../css/celia-default.scss"/>
  <!-- liblouis tables -->
  <uri name="http://www.celia.fi/liblouis/" uri="../liblouis/"
       px:content-type="liblouis-tables" px:id="celia-liblouis-tables"/>
  <!-- hyphen dictionaries-->
  <uri name="http://www.celia.fi/pipeline/hyphen/" uri="../hyph/"
       px:includes="*.dic" px:content-type="libhyphen-tables" px:id="celia-hyphen-tables"/>
  <!-- dependencies -->
  <nextCatalog catalog="org:daisy:pipeline:modules:common-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:file-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:dtbook-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:braille:common-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:braille:css-utils"/>
  <nextCatalog catalog="org:daisy:pipeline:modules:braille:dtbook-to-pef"/>
</catalog>
